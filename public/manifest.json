{
    "manifest_version": 3,
    "name": "Currency Converter",
    "version": "1.0.0",
    "icons": {
        "32": "icon32.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting", "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}